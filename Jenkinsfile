pipeline {
    agent any
    
    stages {
        stage('Build image') {
            steps {
                bat 'echo "Build image"'
            }
        }

        stage('Push image') {
            steps {
                bat 'echo "Push image"'
            }
        }

        stage('Deploy image') {
            steps {
                bat 'echo "Deploy image"'
            }
        }
    }
}
